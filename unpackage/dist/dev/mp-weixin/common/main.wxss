
.container{
	width: 100%;
	height: calc(100vh - 60px);
	overflow: hidden;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
}
.container scroll-view{
	height: 100%;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
}
.container scroll-view .scrollContainer{
	padding: 0rpx 32rpx;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
}
.fixbg{
	position: fixed;
	width: 100%;
	height: 100vh;
	left: 0;
	top: 0;
	/* https://z3.ax1x.com/2021/07/14/WZ6D8P.png */
	/* https://z3.ax1x.com/2021/07/14/WZ6Oa9.png */
	/* background-image:url('https://z3.ax1x.com/2021/07/14/WZ6Oa9.png'); */
	background-position: center;
	background-repeat: no-repeat;
	background-size: cover;
	-webkit-filter: blur(12px);
	        filter: blur(12px);
}

@font-face {
	font-family: "iconfont";
	/* Project id 2670869 */
	src: url(data:font/woff2;base64,) format('woff2'),
		url(data:font/woff;base64,) format('woff'),
		url(data:font/ttf;base64,) format('truetype');
}

.iconfont {
	font-family: "iconfont" !important;
	font-size: 16px;
	font-style: normal;
	vertical-align: middle;
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
	
}

